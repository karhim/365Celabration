<configuration>

    <system.web>
      <compilation debug="true" targetFramework="4.5.2" />
      <httpRuntime targetFramework="4.5.2" />
    </system.web>

    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
            multipleSiteBindingsEnabled="true" />
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IWsServiceCatalog" />
                <binding name="BasicHttpBinding_IWsServiceCatalog1" />
                <binding name="BasicHttpBinding_IWsServiceCatalog2" />
            </basicHttpBinding>
        </bindings>
        <client>           
            <endpoint address="http://localhost:50785/WsServiceCatalog.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWsServiceCatalog2"
                contract="Supplier_WebService.IWsServiceCatalog" name="BasicHttpBinding_IWsServiceCatalog2" />
        </client>
    </system.serviceModel>
</configuration>
